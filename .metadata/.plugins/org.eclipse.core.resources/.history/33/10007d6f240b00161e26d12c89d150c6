public class Wizard{
	Wand Zauberstab;
	String Name;
	
	public Wizard (Wand Zauberstab, String Name){
		this.Zauberstab = Zauberstab;
		this.Name = Name;
	}
	
	public Wizard attack(Wizard other){
		System.out.print ("Der Zauberer\n" + this.getName() + "\ngreift Zauberer\n" + other.getName() + "\nan");
		return this;
	}
	
	public String getName() {
		return this.Name;
	}
}