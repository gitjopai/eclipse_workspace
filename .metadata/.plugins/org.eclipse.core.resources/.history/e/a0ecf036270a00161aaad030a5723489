public class zauberer{
		int power;
		boolean useable;
		public zauberer (int power){
			this.power = power;
		}
		public zauberer (boolean useable){
			this.useable = useable;
		}
	public static void main (String[] args){
		zauberer Erster = new zauberer(0);
		zauberer Zweiter = new zauberer(0);
		zauberer Dritter = new zauberer(0);
		zauberer Vierter = new zauberer(0);
	}
	
}